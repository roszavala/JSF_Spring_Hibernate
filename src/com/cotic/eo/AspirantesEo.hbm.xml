<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cotic.eo.AspirantesEo" table="aspirantes">
		<id name="idAspirante" type="int">
			<column name="idAspirante" precision="22" scale="0" />
			<generator class="identity" />
		</id>
		<property name="nombre" type="string">
			<column name="nombre" length="40" not-null="true" />
		</property>
		<property name="apPaterno" type="string">
			<column name="apPaterno" length="40" not-null="true" />
		</property>
		<property name="apMaterno" type="string">
			<column name="apMaterno" length="40" not-null="true" />
		</property>
		<property name="calle" type="string">
			<column name="calle" length="40" not-null="true" />
		</property>
		<property name="numero" type="string">
			<column name="numero" length="10" not-null="true" />
		</property>
	</class>
</hibernate-mapping>